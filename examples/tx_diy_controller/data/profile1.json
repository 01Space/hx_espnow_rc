		{ "event": { "name": "CHANNEL_EQUAL", "channel": 7, "value": 1000, "once": "yes" }, "op": { "name": "SOUND", "parm": "/disabled.mp3" 	} },
		{ "event": { "name": "CHANNEL_EQUAL", "channel": 7, "value": 1500, "once": "yes" }, "op": { "name": "SOUND", "parm": "/poshold.mp3" } },
		{ "event": { "name": "CHANNEL_EQUAL", "channel": 7, "value": 2000, "once": "yes" }, "op": { "name": "SOUND", "parm": "/rth.mp3" } },

		{ "event": { "name": "CHANNEL_EQUAL", "channel": 8, "value": 1000, "once": "yes" }, "op": { "name": "SOUND", "parm": "/gps_on.mp3" } },
		{ "event": { "name": "CHANNEL_EQUAL", "channel": 8, "value": 2000, "once": "yes" }, "op": { "name": "SOUND", "parm": "/gps_off.mp3"} },

		{ "event": { "name": "CHANNEL_EQUAL", "channel": 9, "value": 875,  "once": "yes" }, "op": { "name": "SOUND", "parm": "/wpm_disabled.mp3" } },
		{ "event": { "name": "CHANNEL_EQUAL", "channel": 9, "value": 1875, "once": "yes" }, "op": { "name": "SOUND", "parm": "/wpm_enabled.mp3"} },

		{ "event": { "name": "CHANNEL_EQUAL", "channel": 10, "value": 1500, "once": "yes" }, "op": { "name": "SOUND", "parm": "/vtx_power_minimum.mp3" 	} },
		{ "event": { "name": "CHANNEL_EQUAL", "channel": 10, "value": 2000, "once": "yes" }, "op": { "name": "SOUND", "parm": "/vtx_power_maximum.mp3" } },

		{ "event": { "name": "CHANNEL_EQUAL", "channel": 11, "value": 1500, "once": "yes" }, "op": { "name": "SOUND", "parm": "/autolaunch_disabled.mp3" } },
		{ "event": { "name": "CHANNEL_EQUAL", "channel": 11, "value": 2000, "once": "yes" }, "op": { "name": "SOUND", "parm": "/autolaunch_enabled.mp3"} },

		{ "event": { "name": "CHANNEL_EQUAL", "channel": 14, "value": 2000, "once": "yes" }, "op": { "name": "SOUND", "parm": "/rec_toggled.mp3"} }
	]
